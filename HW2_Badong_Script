Sub StockTickerHard()      Dim sheet As Worksheet    For Each sheet In ThisWorkbook.Worksheets        sheet.Activate        Range("I1").Value = "Ticker"        Range("J1").Value = "Total Stock Volume"        Range("K1").Value = "Yearly Change ($)"        Range("L1").Value = "Percent Change"        Range("P1").Value = "Ticker"        Range("Q1").Value = "Value"        Range("O3").Value = "Greatest Percent Increase"        Range("O4").Value = "Greatest Percent Decrease"        Range("O5").Value = "Greatest Total Volume"        Dim stock_ticker As String        Dim stock_volume As Double        stock_volume = 0        Dim j As Integer        j = 2        last_row = Cells(Rows.Count, 1).End(xlUp).Row        For i = 2 To last_row            If Cells(i, 1).Value = Cells(i + 1, 1).Value Then                total_stock_volume = stock_volume + Cells(i, 7).Value            ElseIf Cells(i, 1).Value <> Cells(i + 1, 1).Value Then                stock_volume = total_stock_volume + Cells(i, 7).Value                Cells(j, 9).Value = Cells(i, 1).Value                Cells(j, 10).Value = total_stock_volume                j = j + 1                stock_volume = 0            End If        Next i    Next sheetEnd Sub